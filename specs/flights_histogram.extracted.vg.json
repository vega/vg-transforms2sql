{
  "$schema": "https://vega.github.io/schema/vega/v4.json",
  "autosize": "pad",
  "padding": 5,
  "width": 200,
  "height": 200,
  "style": "cell",
  "data": [
    {"name": "source"},
    {
      "name": "data_0",
      "source": "source",
      "transform": [
        {
          "type": "extent",
          "field": "depdelay",
          "signal": "bin_maxbins_10_depdelay_extent"
        },
        {
          "type": "bin",
          "field": "depdelay",
          "as": ["bin_maxbins_10_depdelay", "bin_maxbins_10_depdelay_end"],
          "signal": "bin_maxbins_10_depdelay_bins",
          "maxbins": 10,
          "extent": {"signal": "bin_maxbins_10_depdelay_extent"}
        },
        {
          "type": "aggregate",
          "groupby": ["bin_maxbins_10_depdelay", "bin_maxbins_10_depdelay_end"],
          "ops": ["count"],
          "fields": ["*"],
          "as": ["count_*"]
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "marks",
      "type": "rect",
      "style": ["bar"],
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "fill": [
            {
              "test": "datum[\"bin_maxbins_10_depdelay\"] === null || isNaN(datum[\"bin_maxbins_10_depdelay\"]) || datum[\"count_*\"] === null || isNaN(datum[\"count_*\"])",
              "value": null
            },
            {"value": "#4c78a8"}
          ],
          "tooltip": {
            "signal": "{\"depdelay (binned)\": datum[\"bin_maxbins_10_depdelay\"] === null || isNaN(datum[\"bin_maxbins_10_depdelay\"]) ? \"null\" : format(datum[\"bin_maxbins_10_depdelay\"], \"\") + \" - \" + format(datum[\"bin_maxbins_10_depdelay_end\"], \"\"), \"Count of Records\": format(datum[\"count_*\"], \"\")}"
          },
          "x2": {"scale": "x", "field": "bin_maxbins_10_depdelay", "offset": 1},
          "x": {"scale": "x", "field": "bin_maxbins_10_depdelay_end"},
          "y": {"scale": "y", "field": "count_*"},
          "y2": {"scale": "y", "value": 0}
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "domain": {
        "data": "data_0",
        "fields": ["bin_maxbins_10_depdelay", "bin_maxbins_10_depdelay_end"]
      },
      "range": [0, {"signal": "width"}],
      "zero": false
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {"data": "data_0", "field": "count_*"},
      "range": [{"signal": "height"}, 0],
      "nice": true,
      "zero": true
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "depdelay (binned)",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/10)"},
      "values": {
        "signal": "sequence(bin_maxbins_10_depdelay_bins.start, bin_maxbins_10_depdelay_bins.stop + bin_maxbins_10_depdelay_bins.step, bin_maxbins_10_depdelay_bins.step)"
      },
      "zindex": 1
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Count of Records",
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 1
    },
    {
      "scale": "y",
      "orient": "left",
      "gridScale": "x",
      "grid": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    }
  ],
  "signal": [
    {"name": "bin_maxbins_10_depdelay_extent", "value": []}
  ],
  "config": {"axisY": {"minExtent": 30}}
}
